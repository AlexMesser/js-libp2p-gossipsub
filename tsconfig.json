{
  "compilerOptions": {
    "module": "commonjs",
    "lib": [
      "es6"
    ],
    "target": "es6",
    "noImplicitAny": false,
    "noImplicitThis": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "allowJs": true,
    "outDir": "./src",
    "types": [
      "node"
    ],
  },
  "include": [
    "./ts"
  ]
}
